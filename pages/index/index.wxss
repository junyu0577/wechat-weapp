/**index.wxss**/

page {
  background-color: #f0f0f0;
}

.swiper {
  width: 100%;
  height: 180px;
}

.rankList-list-title-view {
  line-height: 30px;
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 30px;
  font-size: 14px;
  background-color: rgb(255, 255, 255);
  opacity: 0.7;
}

.rankList-list-title {
  color: #262626;
  font-size:14px;
  float:left;
  margin-left:20px
}

.rankList-list-image {
  width: 100%;
  height: 200px;
}

.rankList-list-root-view {
  position: relative;
  text-align: center;
  height: 250px;
}

.rankList-list-view{
  width:100%;
  height:50px;
  line-height:50px
}

.rankList-list-author-icon{
  width:40px;
  height:40px;
  float:left;
  margin-top:5px;
  margin-left:10px;
  border-radius:100px;
}